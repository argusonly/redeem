default: &default
  app_name: 'app-name'
  redis_url: 'redis://127.0.0.1:6379/0'
  admin_nums:
    - 'administrator'
  website:
    title: 'title'
    subtitle: 'subtitle'
    slogan: 'todo'
    favicon: '/images/favicon.ico'
    footer: ''
  unicorn:
    timeout: 30
    worker_processes: 3

production:
  <<: *default

development:
  <<: *default

test:
  <<: *default
